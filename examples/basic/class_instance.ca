class Addable a =
    (+): a -> a -> a
end
instance Addable Int =
    (+) = fun a b -> addInt a b
end
instance Addable (Option (Addable a)) =
    (+) = fun a b ->
        case a of
            | Some(x) -> case b of
                 | Some(y) -> Some (x + y)
                 | None -> None
                 end
            | None -> None
        end
end
in
let x = 2 + 2
in println x